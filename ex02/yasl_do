#!/usr/local/bin yasl

let a = args[1]->toint()
let b = args[3]->toint()
let oper = args[2]

if oper == "+" {
	echo a + b
} elseif oper == "-" {
	echo a - b
} elseif oper == "*" {
	echo a * b
} elseif oper == "/" {
	echo a // b
} elseif oper == "<" {
	echo a < b
} elseif oper == ">" {
	echo a > b
} elseif oper == "<=" {
	echo a <= b
} elseif oper == ">=" {
	echo a >= b
} elseif oper == "==" {
	echo a == b
} elseif oper == "!=" {
	echo a != b
}
