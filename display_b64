#!/yasl
{
	=:1
	"A" >=
	1:= "Z" <=
	&&
	? (1:= "A" - ) : (
		1:= "a" >=
		1:= "z" <=
		&&
		? (1:= "a" - 26 +) : (
			1:= "0" >=
			1:= "9" <= 
			&&
			? (1:= "0" - 52 +) : (
				1:= "+" ==
				? (62) : (
					1:= "/" ==
					? (63)
				)
			)
		)
	)
}convert_b64

{
	read:1
	1:!
	1:][:1
	1:!
	1:!
	0:2
	1:#
	#(
		6:2
		2:<<:2
		1:=:2
		2:=:2
		1:!
		2:|:2
	)
} b64_to_rgb

read
!
][
!!
convert_b64
print
